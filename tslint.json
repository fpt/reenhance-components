{
  "extends": ["tslint-config-airbnb", "tslint-react"],
  "rules": {
    "align": false,
    "import-name": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": false,
    "max-line-length": [true, 120],
    "variable-name": false,
    "only-arrow-functions": true,
    "no-consecutive-blank-lines": [true, 2]
  }
}
