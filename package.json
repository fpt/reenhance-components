{
  "name": "react-operator-nodes",
  "version": "0.0.1",
  "main": "app.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/fpt/reenhance-components"
  },
  "author": "Youichi Fujimoto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fpt/reenhance-components/issues"
  },
  "keywords": [
    "react",
    "components",
    "toolkit",
    "utilities",
    "declarative"
  ],
  "description": "A collection of react nodes which provide functional operation",
  "scripts": {
    "clean": "rm -rf _bundles lib lib-esm",
    "build": "NODE_ENV=production tsc && tsc -m es6 --outDir lib-esm && webpack",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint './**/*.ts{,x}'",
    "start": "NODE_ENV=development webpack-dev-server --port 3000",
    "test": "tsc --noEmit && jest"
  },
  "dependencies": {
    "@types/react": "16.3.8",
    "@types/react-dom": "^16.0",
    "@types/recompose": "^0.26",
    "@types/webpack": "^3.8.1",
    "react": "^16.3",
    "react-dom": "^16.3",
    "recompose": "^0.26.0",
    "rxjs": "^6",
    "ts-loader": "^3.5.0",
    "typescript": "^2.9",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/jest": "^21.1.8",
    "@types/webpack-dev-server": "^2.9.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4",
    "npm-run-all": "^4.1.3",
    "raf": "^3.4.0",
    "ts-jest": "^23.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-react": "^3.2.0",
    "tsutils": "^2.13.0",
    "webpack-dev-server": "^2.9.5"
  },
  "engines": {
    "node": "8.x",
    "yarn": "1.x"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "reenhance-components": "<rootDir>/src/index.ts"
    },
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
